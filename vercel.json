{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.digidiploma.in/api/:path*"
    },
    {
      "source": "/uploads/:path*",
      "destination": "https://api.digidiploma.in/uploads/:path*"
    },
    {
      "source": "/:path((?!assets|icons|.*\\.).*)",
      "destination": "/index.html"
    }
  ]
}
